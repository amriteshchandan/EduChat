<% provide(:title, "Home") %>
<% if user_signed_in? %> 
<div style="margin-left: 2.5%; margin-right:2.5%">
<%= link_to "New Article", new_article_path, class: "btn btn-primary" %><br><br>
	<div class="row">
		<div class="col-md-8">
			  <div class="panel panel-default">
			  	Recent: 
			  	<%= render 'home/feed' %>
			  </div>
		</div>
		
		<div class="col-md-4">
			<!--<div style="border-left:medium #CC0000 solid; height:100%; position:absolute;"></div> -->
			  <div class="panel panel-default">
			  	<%= render 'home/user_may_know' %>
			  </div>
		</div>	
	</div>
	
</div>
<% else %>
<div class="front_page_not_signed_in">
	<h1>Home Page Without Signed IN</h1>	
</div>

<% end %>