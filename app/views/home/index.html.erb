<% if user_signed_in? %> 
<div class="container">
	<div class="row">
		<div class="col-md-8">
		<%= link_to "New Article", new_article_path, class: "btn btn-primary" %>
			  <div class="row">
			  	Recent: 
			  	<%= render 'home/feed' %>
			  </div>
		</div>
		
		<div class="col-md-4">
			<div style="border-left:medium #CC0000 solid; height:100%; position:absolute;"></div>
			  <div class="row">
			  	<%# @users.each do |user| %>
			  		<%# link_to user.name, user_profile_path(user.username) %>
			  	<%# end -%>
			  	<%= render 'home/user_may_know' %>
			  </div>
		</div>	
	</div>
	
</div>
<% else %>
<div class="front_page_not_signed_in">
	<h1>Home Page Without Signed IN</h1>	
</div>

<% end %>